{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/text_overflow.json",
  "title": "TextOverflow",
  "type": "string",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "clip",
        "ellipsis",
        "fade",
        "visible"
      ]
    },
    {
      "type": "string",
      "pattern": "^\\{\\{.*\\}\\}$"
    },
    {
      "type": "string",
      "pattern": "##.*##$"
    }
  ]
}