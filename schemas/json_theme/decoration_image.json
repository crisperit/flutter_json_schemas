{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/decoration_image.json",
  "$comment": "https://api.flutter.dev/flutter/painting/DecorationImage-class.html",
  "title": "DecorationImage",
  "type": "object",
  "properties": {
    "alignment": {
      "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/alignment.json"
    },
    "centerSlice": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/rect.json"
        }
      ]
    },
    "fit": {
      "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/box_fit.json"
    },
    "image": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/image_provider.json"
        }
      ]
    },
    "matchTextDirection": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        },
        {
          "type": "boolean"
        }
      ]
    },
    "repeat": {
      "$ref": "https://peiffer-innovations.github.io/flutter_json_schemas/schemas/json_theme/image_repeat.json"
    },
    "scale": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "number"
        }
      ]
    }
  }
}